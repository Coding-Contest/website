                .flex.flex-col.justify-center
                  %p.text-base.font-medium.leading-normal.line-clamp-1{class: "text-[#1C160C]"} Past Contests
                  %p.text-sm.font-normal.leading-normal.line-clamp-2{class: "text-[#A18249]"} Weekly Contest 291, Sep 26 - Weekly Contest 411, Aug 1
              .shrink-0
                %button.flex.cursor-pointer.items-center.justify-center.overflow-hidden.rounded-full.h-8.px-4.text-sm.font-medium.leading-normal.w-fit{class: "min-w-[84px] max-w-[480px] bg-[#F4EFE6] text-[#1C160C]"}
                  %span.truncate View
          .layout-content-container.flex.flex-col{class: "w-[360px]"}
            %h3.text-lg.font-bold.leading-tight.px-4.pb-2.pt-4{class: "text-[#1C160C] tracking-[-0.015em]"} Global Ranking
            .flex.flex-wrap.gap-3.px-4.py-3
              .flex.flex-1.flex-col.gap-2.rounded-lg.border.p-3.items-start{class: "min-w-[111px] basis-[fit-content] border-[#E9DFCE]"}
