              .flex.flex-col.gap-3.text-center
                .px-4
                  .w-full.bg-center.bg-no-repeat.aspect-square.bg-cover.rounded-full{style: "background-image: url(\"https://cdn.usegalileo.ai/sdxl10/1ad86168-e858-4195-8ee9-8a656ebd20e4.png\");"}
              .flex.flex-col.gap-3.text-center
                .px-4
                  .w-full.bg-center.bg-no-repeat.aspect-square.bg-cover.rounded-full{style: "background-image: url(\"https://cdn.usegalileo.ai/sdxl10/478b0114-72a5-4abd-bbd6-2b06949729d2.png\");"}
